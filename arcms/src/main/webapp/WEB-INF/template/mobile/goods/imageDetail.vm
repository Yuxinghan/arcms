<script type="text/javascript" charset="utf-8" src="${rc.contextPath}/media/mobile/js/jquery.raty.min.js"></script>
<script type="text/javascript">
    $(function() {
        $('.star').raty({ readOnly: true, score: 3 ,size:15,starOn:"o-star-on.png"});
    });
</script>

#set($layout = "/layout/mobileDefault.vm")
<link href="${rc.contextPath}/media/css/trade/reset.css" rel="stylesheet" type="text/css" />
<link href="${rc.contextPath}/media/css/trade/style.css" rel="stylesheet" type="text/css" />

<head>
    <title>
        绑定会员卡
    </title>
</head>
<script type="text/javascript" src="${rc.contextPath}/media/js/user/user.js"></script>
<section class="wrap">
    <section class="wrap">
        <section class="pages">
            <div class="login">
                <form class="form-horizontal" role="form" id="memberCardBindForm" method="post">
                    <ul>
                        <li class="binding">
                            <input name="memberCard" id="memberCard" value="$!{user.memberCard}" type="text" class="inp" placeholder="请绑定卡号">
                        </li>
                    </ul>
                    <div class="submit">
                        <button type="button" class="br20 combtn" onclick="bindMemberCard()">提 交</button>
                    </div>
                </form>
            </div>
        </section>
    </section>
</section>
